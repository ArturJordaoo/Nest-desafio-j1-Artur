generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pet {
  id         String   @id @default(uuid())
  nome       String
  nascimento DateTime
  animal     String
  raca       String
  donoId     String
  dono       Dono     @relation(fields: [donoId], references: [id])
}

model Dono {
  id       String @id @default(uuid())
  donoNome String
  telefone String
  pets     Pet[]
}
